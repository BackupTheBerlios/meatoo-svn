
<table><tr>
<td width="16%">
#if not $my_herds
    <font size=-1>
    <a href='/meatoo/login'>Login</a><br><br>
    <a href='/meatoo/signup'>New Account</a><br><br>
    <a href='/meatoo/lost_passwd'>Lost password?</a>
    </font>
#end if
#if $my_herds
<br>
<b>My Herds</b><a href="/meatoo/add_user_herd">[+]</a><br />
#for $my_h in $my_herds
    <a href="/meatoo/search/short/$my_h/herd">$my_h</a>
    <a href="/meatoo/del_user_herd/$my_h"><img border=0 src="/meatoo/static/delete.png"></a>    
    <br>
#end for
#end if
#if $username
<br><b>My Troves</b><a href="/meatoo/add_user_trove">[+]</a>
#end if
#if $troves
<br />
<a href="/meatoo/show_all_troves">All</a><br />
#for $t, $n in $troves.items()
<a href="/meatoo/show_trove/$t"><span title="$n">$t</span></a>
<a href="/meatoo/del_user_trove/$t"><img border=0 src="/meatoo/static/delete.png"></a>
<br />
#end for
#end if
</td>
<td width="55%"><table>
#for $pkg in $packages
 #if $pkg.latestReleaseDate == $week[0] or $pkg.latestReleaseDate == $week[1]
    <tr class="alt">
    <td>

    #if $verbose
        ($pkg.id)
    #end if    

    <b><a class="nav" 
    href="http://packages.gentoo.org/search/?sstring=%5E$pkg.packageName%24" 
    title="Latest portage version">
    $pkg.portageCategory/$pkg.packageName-$pkg.portageVersion</a>
    [ <a class="nav" 
    href="http://freshmeat.net/projects/$pkg.packageName/" 
    title="Freshmeat Latest Release">$pkg.latestReleaseVersion</a> ]</b>
    &nbsp;&nbsp;&nbsp;
    <a class="nav" 
    href="/meatoo/ignore/$pkg.packageName/$pkg.latestReleaseVersion" 
    title="Ignore this version of this package.">
    <img border=0 src="/meatoo/static/edit.png" alt="Ignore"></a>
    <a class="nav" href="/meatoo/add_known/$pkg.fmName" title="Edit Portage name">
    <img border=0 src="/meatoo/static/edit.gif"></a>
    <a class="nav" href="$pkg.urlHomepage" title="Project Homepage">
    <img border=0 src="/meatoo/static/home.png"></a>

    <a class="nav" 
    href="http://freshmeat.net/redir/$pkg.packageName/$pkg.urlChangelog/"
    title="Upstream ChangeLog">
    <img border=0 src="/meatoo/static/changelog.gif" alt="changelog"></a>

    </td></tr>
<tr><td>
    <p><font size=-1>$pkg.latestReleaseDate</font></p>
    <b>Portage description:</b><br>
    $pkg.portageDesc<br><br>
    <b>Freshmeat description:</b><br>
    $pkg.descShort<br><br>
    <b>Maintainers:</b> $pkg.maintainerName<br><br><hr></td></tr>
 #end if
#end for
</table>
</td>
<td>
<table>
#set $i = 0
#while $i < 5
    <tr class="alt"><td><b>$week[$i]</b></tr>
    #for $pkg in $packages
        #if $pkg.latestReleaseDate == $week[$i]
            <tr><td>
            <a class="nav"
            href="/meatoo/details/$pkg.portageCategory/$pkg.packageName"
            title="Package details">
            $pkg.portageCategory/$pkg.packageName-$pkg.portageVersion</a>

            <a class="nav"
            href="/meatoo/details/$pkg.portageCategory/$pkg.packageName"
            title="Package details">
            [ $pkg.latestReleaseVersion ]</a>
            </td>
            </tr>
        #end if
    #end for
    #set $i += 1 
#end while
</table></td></tr></table>

